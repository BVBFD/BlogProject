    "@types/node": "18.17.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "antd": "^5.5.1",
    "autoprefixer": "10.4.14",
    "axios": "^1.2.1",
    "critters": "^0.0.20",
    "eslint": "^8.39.0",
    "eslint-config-next": "13.3.2",
    "highlight.js": "^11.7.0",
    "next": "13.4.12",
    "next-seo": "^6.4.0",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-quill": "^2.0.0",
    "react-redux": "^8.0.5",
